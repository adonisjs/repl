{
  "name": "@adonisjs/repl",
  "description": "REPL for AdonisJS",
  "version": "4.0.0-0",
  "main": "build/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.16.0"
  },
  "files": [
    "build/src",
    "build/factories",
    "build/index.d.ts",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types/main.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "lint": "eslint . --ext=.ts",
    "clean": "del-cli build",
    "typecheck": "tsc --noEmit",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels .github/labels.json adonisjs/repl",
    "quick:test": "node --enable-source-maps --loader=ts-node/esm bin/test.ts",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.1.5-4",
    "@adonisjs/eslint-config": "^1.1.7",
    "@adonisjs/prettier-config": "^1.1.7",
    "@adonisjs/tsconfig": "^1.1.7",
    "@japa/assert": "^2.0.0-1",
    "@japa/file-system": "^2.0.0-1",
    "@japa/runner": "^3.0.0-2",
    "@swc/core": "^1.3.67",
    "@types/node": "^20.3.3",
    "c8": "^8.0.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.44.0",
    "github-label-sync": "^2.3.1",
    "husky": "^8.0.3",
    "np": "^8.0.4",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@poppinss/colors": "4.1.0-2",
    "string-width": "^6.1.0"
  },
  "author": "virk,julien-r44,adonisjs",
  "license": "MIT",
  "homepage": "https://github.com/adonisjs/repl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/repl.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/repl/issues"
  },
  "keywords": [
    "adonisjs",
    "repl",
    "node-repl"
  ],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "next",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  }
}
